<configuration>
  <configSections>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
      <section name="Crypt.Windows.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    </sectionGroup>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
      <section name="Crypt.Windows.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
    </sectionGroup>
  </configSections>

  <applicationSettings>
    <Crypt.Windows.Properties.Settings>
   <setting name="SupportedLanguages" serializeAs="Xml">
    <value>
     <ArrayOfString xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xmlns:xsd="http://www.w3.org/2001/XMLSchema">
      <string>en</string>
      <string>fr</string>
     </ArrayOfString>
    </value>
   </setting>
   <setting name="WebSite" serializeAs="String">
    <value>https://github.com/cedx/crypt.cs</value>
   </setting>
   <setting name="BugTracker" serializeAs="String">
    <value>https://github.com/cedx/crypt.cs/issues</value>
   </setting>
   <setting name="TechnicalSupport" serializeAs="String">
    <value>https://github.com/cedx/crypt.cs/wiki</value>
   </setting>
  </Crypt.Windows.Properties.Settings>
  </applicationSettings>

  <userSettings>
    <Crypt.Windows.Properties.Settings>
   <setting name="Culture" serializeAs="String">
    <value>(Default)</value>
   </setting>
  </Crypt.Windows.Properties.Settings>
  </userSettings>
</configuration>